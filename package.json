{
  "name": "getx-park",
  "displayName": "GetX Park",
  "description": "extracts getx variable from fuzzy-code",
  "version": "0.0.2",
  "publisher": "jinnavat",
  "repository": {
    "type": "git",
    "url": "https://github.com/s0ngkran/getx-park"
  },
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["onStartupFinished"],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "getx-park.showObxFull",
        "title": "GetX Park: full screen; check Obx; getx park"
      },
      {
        "command": "getx-park.showObxHalf",
        "title": "GetX Park: half screen; check Obx; getx park"
      }
    ],
    "keybindings": [
      {
        "command": "getx-park.showObxHalf",
        "key": "ctrl+shift+z",
        "mac": "cmd+shift+z",
        "when": "editorTextFocus"
      },
      {
        "command": "getx-park.showObxFull",
        "key": "ctrl+shift+alt+z",
        "mac": "cmd+shift+ctrl+z",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.77.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.3.0"
  }
}
